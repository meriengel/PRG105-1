def main():
    try: numbers_file = open("numbers.txt", "r")
    except exception as errorGenerated:
    print("file not found:", errorGenerated)
    else:
    total = 0
    number_of_lines = 0
    line = numbers_file.readline()


while: line != "":
number_of_lines += 0
total += int(line)
line = numbers_file.readline()
average = total / number_of_lines

print("the number is 18")
print("the total is, total")
print("the average is, average")

main()
    
    
